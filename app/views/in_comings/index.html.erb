<%- model_class = InComing -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:house_id) %></th>
      <th><%= model_class.human_attribute_name(:params) %></th>
      <th><%= model_class.human_attribute_name(:update_reason) %></th>
      <th><%= model_class.human_attribute_name(:house_unit_id) %></th>
      <th><%= model_class.human_attribute_name(:ambient_temp) %></th>
      <th><%= model_class.human_attribute_name(:temp) %></th>
      <th><%= model_class.human_attribute_name(:ct1_realPower) %></th>
      <th><%= model_class.human_attribute_name(:ct2_realPower) %></th>
      <th><%= model_class.human_attribute_name(:ct3_realPower) %></th>
      <th><%= model_class.human_attribute_name(:ct4_realPower) %></th>
      <th><%= model_class.human_attribute_name(:ct1_Vrms) %></th>
      <th><%= model_class.human_attribute_name(:time_stamp) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @in_comings.each do |in_coming| %>
      <tr>
        <td><%= link_to in_coming.id, in_coming_path(in_coming) %></td>
        <td><%= in_coming.house_id %></td>
        <td><%= in_coming.params %></td>
        <td><%= in_coming.update_reason %></td>
        <td><%= in_coming.house_unit_id %></td>
        <td><%= in_coming.ambient_temp %></td>
        <td><%= in_coming.temp %></td>
        <td><%= in_coming.ct1_realPower %></td>
        <td><%= in_coming.ct2_realPower %></td>
        <td><%= in_coming.ct3_realPower %></td>
        <td><%= in_coming.ct4_realPower %></td>
        <td><%= in_coming.ct1_Vrms %></td>
        <td><%= in_coming.time_stamp %></td>
        <td><%=l in_coming.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_in_coming_path(in_coming), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      in_coming_path(in_coming),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_in_coming_path,
            :class => 'btn btn-primary' %>
